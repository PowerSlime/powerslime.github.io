.container.padding-50-on-top
	.row
		//- Skills
		.col-md-6.skills-block
			h2(data-translate-id="skills-header")

			section.container.skills-container.container-with-no-padding
			.row
				.col-md-12
					table.skills-table
						+add_skill('JavaScript (ES5/ES6)', [4, 5])
						+add_skill('React', [2, 5])
						+add_skill('Python', [3, 5])

					table.skills-table
						+add_skill('HTML/CSS', [4, 5])
						+add_skill('Bootstrap', [4, 5])
						+add_skill('jQuery', [4, 5])

					table.skills-table
						+add_skill('Jade (Pug)', [4, 5])
						+add_skill('SASS', [4, 5])

					table.skills-table
						+add_skill('Git', [4, 5])
						+add_skill('StackOverflow', [5, 5])

		//- Interests
		.col-md-6.interests-block.text-align-to-right
			h2(data-translate-id="interests-header")

			section.container.interests-container.container-with-no-padding
			.row
				.col-md-12
					.custom-flex
						ul.interests-list
							+add_interest('./img/magic.jpg', 'Click ^_^', 'interests-anime')(custom_class='magic', custom_text='Anime')
							+add_interest('./img/photography.jpg', 'Photography', 'interests-photography')
							+add_interest('./img/design.jpg', 'Design', 'interests-design')
							+add_interest('./img/programming.jpg', 'Programming', 'interests-programming')
